      <wsdl:input>
        <soap12:body use="literal" />
      </wsdl:input>
      <wsdl:output>
        <soap12:body use="literal" />
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="CustomSTS">
    <wsdl:port name="WSHttpBinding_SecurityTokenService" binding="tns:WSHttpBinding_SecurityTokenService">
      <soap12:address location="https://localhost:44368/CustomSTS.svc/CustomSTS" />
      <wsa10:EndpointReference>
        <wsa10:Address>https://localhost:44368/CustomSTS.svc/CustomSTS</wsa10:Address>
      </wsa10:EndpointReference>
    </wsdl:port>
  </wsdl:service>
